{
  "name": "agentstatic",
  "version": "0.1.0",
  "description": "AI-first static site generator for brochureware marketing websites",
  "type": "module",
  "scripts": {
    "start": "storybook dev -p 6006",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "precompile:templates": "nunjucks-precompile -f -i \"\\.njk$\" components > .storybook/precompiled-templates.js",
    "storybook": "bun run precompile:templates && storybook dev -p 6006",
    "build-storybook": "bun run precompile:templates && storybook build",
    "test": "bun test",
    "test:a11y": "echo 'Accessibility tests not yet configured' && exit 0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^10.0.1",
    "@storybook/html": "^10.0.1",
    "@storybook/html-vite": "^10.0.1",
    "storybook": "^10.0.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "nunjucks": "^3.2.4"
  }
}
